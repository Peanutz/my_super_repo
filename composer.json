{
    "name": "zendframework/zendframework1",
    "description": "Zend Framework 1",
    "type": "library",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.2.11",
	"zend/acl": "dev-master",
	"zend/application": "dev-master",
	"zend/auth": "dev-master",
	"zend/cache": "dev-master",
	"zend/config": "dev-master",
	"zend/date": "dev-master",
	"zend/db": "dev-master",
	"zend/debug": "dev-master",
	"zend/form": "dev-master",
	"zend/layout": "dev-master",
	"zend/log": "dev-master",
	"zend/mail": "dev-master",
	"zend/measure": "dev-master",
	"zend/paginator": "dev-master",
	"zend/session": "dev-master",
	"zend/version": "dev-master",
	"zend/view": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Zend_": "library/"
        }
    },
    "include-path": [
        "library/"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.12.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.3.*"
    },
    "archive": {
        "exclude": ["/demos", "/documentation", "/tests"]
    }
}
